		"precommit": "lint-staged",
		"prettier": "prettier --write \"**/*.{js,jsx}\"",
		"electron": "electron ./src/start.js"
	},
	"devDependencies": {
		"babel-eslint": "^8.2.3",
		"eslint": "^4.19.1",
		"eslint-config-airbnb": "^16.1.0",
