		"eslint-plugin-import": "^2.11.0",
		"eslint-plugin-jsx-a11y": "^6.0.3",
		"eslint-plugin-react": "^7.7.0",
		"husky": "^0.14.3",
		"lint-staged": "^7.0.4",
		"prettier": "^1.12.1"
	},
	"prettier": {
		"printWidth": 100,
		"singleQuote": true,
		"useTabs": true
	},
	"lint-staged": {
		"*.{js,jsx}": [
			"prettier --write",
			"eslint --fix",
			"git add"
