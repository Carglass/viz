{
	"name": "viz",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"electron": "^1.8.4",
		"react": "^16.3.2",
		"react-dom": "^16.3.2",
		"react-scripts": "1.1.4",
		"react-toolbox": "^2.0.0-beta.12"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test --env=jsdom",
